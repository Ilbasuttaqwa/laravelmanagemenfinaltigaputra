{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "functions": {
    "public/index.php": {
      "runtime": "@vercel/php"
    }
  },
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false"
  },
  "build": {
    "env": {
      "COMPOSER_HOME": "/tmp"
    }
  }
}
